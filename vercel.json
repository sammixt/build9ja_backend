{
    "version": 2,
     "builds": [
        { 
            "src": "./server.js", 
            "use": "@vercel/node" 
        }
      ],
    "routes": [
        {
            "src": "/(.*)\\.(png|jpg|jpeg)$",
            "dest": "/$1.$2"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}
